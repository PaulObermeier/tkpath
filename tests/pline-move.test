package require tkpath; # 0.2.4 cairo & aqua pack [tkp::canvas .c] if {[lsearch [namespace children] ::tcltest] == -1} {     package require tcltest     namespace import -force ::tcltest::* }test pline-1.1 {poline coords} {     .c coords [.c create pline 200 201 250 202] } {200.0 201.0 250.0 202.0} test pline-1.2 {pline {coords after move}} {     set pline [.c create pline 200 201 250 202]     .c move $pline 20 20     .c coords $pline } {220.0 221.0 270.0 222.0} 